extends ../layout/common

block variables
  - var title = 'Страница 404'
  - var pageClass = 'unknown'

block content
  section.breadcrumbs
    .container
      ul.breadcrumbs-list
        li.breadcrumbs-list__item
          a.breadcrumbs-list__link(href="javascript:void(0);") Главная

  section.unknown
    .container
      .unknown-content
        h1.g-page-title Ошибка 404
        h3.g-block-title Такой страницы не существует
        p.unknown-text  Lorem ipsum dolor sit amet, consectetur adipisicing elit.
          =' '
          a.unknown-text__link(href="javascript:void(0);") Hic sequiaspernatur
          |  Lorem ipsum dolor
          =' '
