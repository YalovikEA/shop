          |  Lorem ipsum dolor sit amet, consectetur adipisicing elit. Labore, voluptate.
        ul.unknown-list
          each link in ['мебель','бытовая техника','хранение вина','посуда','ножи','хранение вина','посуда','мебель','бытовая техника']
            li.unknown-list__item
              a.unknown-list__link(href="javascript:void(0);")= link
        form.unknown-form
          .g-form-block.unknown-form__input
            input.g-input(type="text")
          .g-form-block.unknown-form__submit
            input.g-btn.g-btn--black.g-btn--no-underline(type="submit" value="Найти")
